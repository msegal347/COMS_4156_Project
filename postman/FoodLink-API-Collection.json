                { "key": "Authorization", "value": "Bearer {{token}}" }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\"amount\":200}"
              }
            }
          },
          {
            "name": "Delete Allocation by ID",
            "request": {
              "method": "DELETE",
              "url": "{{baseUrl}}/api/allocations/:id",
              "header": [
                { "key": "Authorization", "value": "Bearer {{token}}" }
              ]
