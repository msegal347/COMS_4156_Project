        "name": "Allocation",
        "item": [
          {
            "name": "Create Allocation",
            "request": {
              "method": "POST",
              "url": "{{baseUrl}}/api/allocations/",
              "header": [
                { "key": "Content-Type", "value": "application/json" }
              ],
              "body": {
                "mode": "raw",
                "raw": "{\"name\":\"Allocation1\",\"resource\":\"Resource1\",\"amount\":100}"
              }
            }
          },
          {
            "name": "Get Allocation by ID",
            "request": {
              "method": "GET",
              "url": "{{baseUrl}}/api/allocations/:id",
              "header": [
                { "key": "Authorization", "value": "Bearer {{token}}" }
              ]
            }
          },
          {
            "name": "Update Allocation by ID",
            "request": {
              "method": "PUT",
              "url": "{{baseUrl}}/api/allocations/:id",
              "header": [
